{
  "name": "comcom",
  "version": "1.0.1",
  "description": "A simple and easy to use comment to comment parser",
  "main": "dst/lib/index.js",
  "scripts": {
    "babel": "babel --out-dir dst src",
    "lint": "eslint src/lib/index.js",
    "test": "tap --cov -R spec dst/tst/index.js",
    "pretest": "npm run lint; npm run babel",
    "readme": "documentation readme ./src/lib/index.js --readme-file Readme.md -s \"...\"",
    "prereadme": "mv .babelrc babelrc",
    "postreadme": "mv babelrc .babelrc",
    "prepublish": "npm t && npm run readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/comcom.git"
  },
  "keywords": [
    "comment",
    "comment-parser",
    "comcom",
    "comment-comment",
    "comment-to-comment"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/comcom/issues"
  },
  "homepage": "https://github.com/mrzmmr/comcom#readme",
  "devDependencies": {
    "eslint": "^1.10.3"
  },
  "dependencies": {
    "defop": "^1.1.4",
    "string.prototype.startswith": "^0.2.0"
  }
}
