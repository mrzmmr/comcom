{
  "name": "comcom",
  "version": "1.0.6",
  "description": "A simple and easy to use comment to comment parser",
  "main": "dst/lib/index.js",
  "scripts": {
    "build": "npm run clean; npm t; npm run readme",
    "clean": "rm -rf dst .nyc_output",
    "babel": "babel --out-dir dst src",
    "lint": "eslint src/lib/index.js",
    "test": "tap --cov -R spec dst/tst/index.js",
    "pretest": "npm run lint; npm run babel",
    "readme": "documentation readme ./src/lib/index.js --readme-file Readme.md -s \"...\"",
    "prereadme": "mv .babelrc babelrc",
    "postreadme": "mv babelrc .babelrc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/comcom.git"
  },
  "keywords": [
    "comment",
    "comment-parser",
    "comcom",
    "comment-comment",
    "comment-to-comment"
  ],
  "author": "Paul Zimmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/comcom/issues"
  },
  "homepage": "https://github.com/mrzmmr/comcom#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.4.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "documentation": "github:documentationjs/documentation",
    "eslint": "^1.10.3",
    "tap": "^5.0.0"
  },
  "dependencies": {
    "defop": "^1.1.4",
    "source-map-support": "^0.4.0",
    "string.prototype.endswith": "^0.2.0",
    "string.prototype.startswith": "^0.2.0",
    "through": "^2.3.8"
  }
}
